<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposePracticeDemoApp"
        tools:targetApi="31">
        <activity
            android:name=".InstaProfile"
            android:exported="false"
            android:label="@string/title_activity_insta_profile"
            android:theme="@style/Theme.ComposePracticeDemoApp" />
        <activity
            android:name=".MeditationUi"
            android:exported="false"
            android:label="@string/title_activity_meditation_ui"
            android:theme="@style/Theme.ComposePracticeDemoApp" />
        <activity
            android:name=".DraggableMusicKnob"
            android:exported="false"
            android:label="@string/title_activity_draggable_music_knob"
            android:theme="@style/Theme.ComposePracticeDemoApp" />
        <activity
            android:name=".CircularProgressBar"
            android:exported="false"
            android:label="@string/title_activity_circular_progress_bar"
            android:theme="@style/Theme.ComposePracticeDemoApp" />
        <activity
            android:name=".SimpleAnimation"
            android:exported="false"
            android:label="@string/title_activity_simple_animation"
            android:theme="@style/Theme.ComposePracticeDemoApp" />
        <activity
            android:name=".HomeScreen"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposePracticeDemoApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListCompose"
            android:exported="false"
            android:label="@string/title_activity_list_compose"
            android:theme="@style/Theme.ComposePracticeDemoApp" />
        <activity
            android:name=".BasicFields"
            android:exported="false"
            android:label="@string/title_activity_basic_fields"
            android:theme="@style/Theme.ComposePracticeDemoApp" />
        <activity
            android:name=".BasicStateHandle"
            android:exported="false"
            android:label="@string/title_activity_basic_state_handle"
            android:theme="@style/Theme.ComposePracticeDemoApp" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="@string/main_activity"
            android:theme="@style/Theme.ComposePracticeDemoApp" />
    </application>

</manifest>